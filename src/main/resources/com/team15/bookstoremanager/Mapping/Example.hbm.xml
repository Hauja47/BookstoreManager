<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.homework.examschedulemng.models.Phancong" table="phancong" schema="exam">
        <id name="id">
            <column name="id" sql-type="long"/>
        </id>
        <property name="subject_id">
            <column name="subject_id" sql-type="string"/>
        </property>
        <property name="subject_name">
            <column name="subject_name" sql-type="string"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="shift">
            <column name="shift" sql-type="int"/>
        </property>
        <property name="room">
            <column name="room" sql-type="string"/>
        </property>
        <property name="no_student">
            <column name="no_student" sql-type="int"/>
        </property>
        <property name="no_proctor">
            <column name="no_proctor" sql-type="int"/>
        </property>
        <property name="is_pass">
            <column name="is_pass" sql-type="boolean"/>
        </property>
    </class>
</hibernate-mapping>